{
    "manifest_version": 2,
    "name": "Anti Phishing P.Singh",
    "version": "1.0.0",
    "background": {
        "persistent": true,
        "scripts": ["background.ts"]
    },
    "content_scripts": [
        {
            "js": ["content.ts"],
            "matches": ["<all_urls>"]
        }
    ],

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "notifications",
        "alarms",
        "tabs",
        "activeTab",
        "storage",
        "<all_urls>"
    ]
}
